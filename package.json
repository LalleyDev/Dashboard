{
  "name": "dashboard",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "concurrently \"npm run backend\" \"npm run frontend\"",
    "frontend": "cd frontend/dist && vite",
    "backend": "cd backend/dist && nodemon index.js",
    "build": "concurrently \"npm run buildBackend\" \"npm run buildFrontend\"",
    "buildFrontend": "cd frontend && vite build && tsc",
    "buildBackend": "cd backend && tsc && npm run postBuildBackend",
    "postBuildBackend": "cd backend && copyfiles -u 1 src/links.json dist",
    "preview": "cd frontend/dist && vite preview"
  },
  "devDependencies": {
    "@types/axios": "^0.9.36",
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.3",
    "@types/node": "^24.1.0",
    "concurrently": "^9.0.2",
    "eslint": "^9.32.0",
    "prettier": "^3.6.2",
    "vite": "^7.0.2"
  },
  "dependencies": {
    "axios": "^1.11.0",
    "copyfiles": "^2.4.1",
    "cors": "^2.8.5",
    "express": "^5.1.0",
    "nodemon": "^3.1.10",
    "ts-node": "^10.9.2",
    "typescript": "^5.8.3"
  }
}
